{
    "version": 2,
    "builds": [
        {
            "src": "backend/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/backend",
            "methods": "GET,OPTIONS,PATCH,DELETE,POST,PUT",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}